{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "api/rss.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/rss", "dest": "/api/rss.js" },
    { "src": "/images/(.*)", "dest": "/images/$1" },
    { "src": "/js/(.*)", "dest": "/js/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
